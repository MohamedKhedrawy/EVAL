{
  "version": 2,
  "routes": [
    { "src": "/api/users/register", "dest": "/api/index.js" },
    { "src": "/api/users/login", "dest": "/api/index.js" },
    { "src": "/api/users/myinfo", "dest": "/api/index.js" },
    { "src": "/api/questions/.*", "dest": "/api/index.js" },
    { "src": "/api/questions/clear", "dest": "/api/index.js" },
    { "src": "/api/questions/wrong", "dest": "/api/index.js" }
  ],
  "functions": {
    "api/index.js": { "runtime": "nodejs18.x" }
  }
}